
// react scripts modifies this file automatically. For webpack configuration noEmit should be set to false.
{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "esnext",
    "target": "es5",
    "lib": [
      "es2015",
      "dom",
      "es2017"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "noFallthroughCasesInSwitch": true,
    "noEmit": true
  },
  "include": [
    "./src",
    "./server",
    "./src/ts/**/*"
  ],
  "exclude": [
    "./node_modules"
  ]
}
